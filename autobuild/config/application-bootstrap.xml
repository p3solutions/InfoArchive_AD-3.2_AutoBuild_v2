<?xml version="1.0" encoding="UTF-8"?>
<bootstrap>
  <name>medtronics</name>
  <mainStore class="XDBStore">
    <alias>main</alias>
    <defaultStoreUser class="XDBStoreUser">
      <id>Administrator</id>
    </defaultStoreUser>
    <bootstrap>xhive://localhost:1235#medtronics</bootstrap>
    <databaseName>medtronics</databaseName>
    <cachePages>10000</cachePages>
  </mainStore>
  <defaultURIResolver class="DDSURIResolver" />
  <publicKeyPath>./DDSPublicKey.dat</publicKeyPath>
  <privateKeyPath>./DDSPrivateKey.dat</privateKeyPath>
  <jceSettings>
    <crypto>
      <transformation>RSA/ECB/OAEPWithSHA-1AndMGF1Padding</transformation>
      <provider></provider>
    </crypto>
    <secureRandom>
      <algorithm>SHA1PRNG</algorithm>
      <provider></provider>
    </secureRandom>
  </jceSettings>
</bootstrap>
